ge>\r\n      <el-badge :value=\"2\" class=\"item\" type=\"warning\">\r\n        <el-button size=\"small\">收藏</el-button>\r\n      </el-badge>\r\n    </article>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import Qs from \"qs\"\r\n  export default {\r\n    name: \"Latest\",\r\n    data(){\r\n      return {\r\n        isFavorite:false,\r\n        isCollection:false,\r\n        topics:[],\r\n        favorites:[]\r\n      }\r\n    },\r\n    mounted() {\r\n      this.$axios.post('http://localhost:8080/topic/latestTopic').then(\r\n          response => {\r\n            this.topics = response.data\r\n          },\r\n      )\r\n      this.$axios.post('http://localhost:8080/favorite/selectFavorite').then(\r\n          response => {\r\n            /*let favorite\r\n            for (favorite in response.data){\r\n              console.log(favorite)\r\n            }*/\r\n            console.log(typeof response.data[0])\r\n          },\r\n          error => {\r\n            console.log(error)\r\n          }\r\n      )\r\n    },\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  .item {\r\n    margin-top: 10px;\r\n    margin-right: 40px;\r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module"}